#: ../src/billreminder.py:33
msgid ""
"Usage:  billreminder [OPTIONS...]\n"
"\n"
"Options:\n"
"  --help, -h, -?\tShow this message.\n"
"  --about\t\tAbout this application.\n"
"  --add\t\t\tAdds a new record to the database.\n"
"  --standalone\t\tAccess database directly, without daemon.\n"
"  --version, -v\t\tDisplays the version number for this application.\n"
msgstr ""
"Utilisation :  billreminder [OPTIONS...]\n"
"\n"
"Options :\n"
"  --help, -h, -?\tAffiche ce message.\n"
"  --about\t\tÀ propos de cette application.\n"
"  --add\t\t\tAjoute un nouvel renregistrement à la base de données.\n"
"  --standalone\t\tAccès direct à la base de données, sans le démon.\n"
"  --version, -v\t\tAffiche le numéro de version de cette application.\n"

#: ../src/billreminder.py:52
#: ../src/billreminderd.py:47
#: ../src/daemon/dbus_manager.py:44
#, python-format
msgid "This is %(appname)s - Version: %(version)s"
msgstr "Application : %(appname)s - Version : %(version)s"

#: ../src/billreminderd.py:21
msgid ""
"Usage:  billreminderd [OPTIONS...]\n"
"\n"
"Options:\n"
"  --help, -h, -?\tShow this message.\n"
"  --verbose\t\tPrint output messages.\n"
"  --no-daemon\t\tDon't run as a daemon.\n"
"  --open-gui\t\tStart daemon and launch GUI.\n"
"  --stop\t\t\tStop daemon.\n"
"  --version, -v\t\tDisplays the version number for this application.\n"
msgstr ""
"Utilisation :  billreminderd [OPTIONS...]\n"
"\n"
"Options :\n"
"  --help, -h, -?\tAffiche ce message.\n"
"  --verbose\t\tAffiche les messages de sortie.\n"
"  --no-daemon\t\tNe pas lancer en tant que démon.\n"
"  --open-gui\t\tDémarre démon et lance l'interface graphique.\n"
"  --stop\t\t\tArrête le démon.\n"
"  --version, -v\t\tAffiche le numéro de version de cette application.\n"

